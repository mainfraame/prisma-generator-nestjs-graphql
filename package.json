{
  "name": "prisma-generator-nestjs-graphql-crud",
  "description": "generates basic graphql crud classes for @nestjs/graphql",
  "version": "2.1.0",
  "bin": "./dist/prisma-generator-nestjs-graphql-crud.js",
  "license": "MIT",
  "author": "dayne.mentier@gmail.com",
  "keywords": [
    "@nestjs/graphql",
    "graphql",
    "graphql-yoga",
    "nestjs",
    "prisma",
    "dto",
    "resolvers",
    "generator"
  ],
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "node ./build.js && yarn build:shebang && yarn build:chmod",
    "build:chmod": "chmod +x ./dist/prisma-generator-nestjs-graphql-crud.js",
    "build:shebang": "echo \"#!/usr/bin/env node\"|cat - ./dist/prisma-generator-nestjs-graphql-crud.js > /tmp/out && mv /tmp/out ./dist/prisma-generator-nestjs-graphql-crud.js",
    "casing": "yarn prisma-case-format --file=./public.prisma",
    "decheck": "yarn depcheck .",
    "dev": "yarn build && yarn gen",
    "format": "yarn prisma format --schema=./public.prisma",
    "gen": "prisma generate --schema=./public.prisma",
    "lint": "eslint --ignore-path ./.eslintignore . --quiet --fix",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@prisma/generator-helper": "^5.2.0",
    "@swc/core": "^1.3.82",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@nestjs/common": "^10.2.4",
    "@nestjs/core": "^10.2.3",
    "@nestjs/graphql": "^12.0.8",
    "@prisma/client": "^5.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "depcheck": "^1.4.5",
    "esbuild": "^0.19.2",
    "esbuild-node-externals": "^1.9.0",
    "eslint": "^8.48.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "graphql": "^16.8.0",
    "graphql-scalars": "^1.22.2",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "prisma": "^5.2.0",
    "prisma-case-format": "^1.7.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@nestjs/core": "^10.2.3",
    "@nestjs/graphql": "^12.0.8",
    "graphql": "^16.8.0",
    "graphql-scalars": "^1.22.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "modulePathIgnorePatterns": [
      "__helpers__/",
      "__fixtures__/"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "auto",
    "htmlWhitespaceSensitivity": "strict",
    "importOrder": [
      "^@(.*)/(.*)$",
      "<THIRD_PARTY_MODULES>",
      "^[./]"
    ],
    "importOrderCaseInsensitive": true,
    "importOrderParserPlugins": [
      "decorators-legacy",
      "jsx",
      "tsx",
      "typescript"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "insertPragma": false,
    "jsxSingleQuote": true,
    "quoteProps": "as-needed",
    "requirePragma": false,
    "proseWrap": "preserve",
    "semi": true,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "packageManager": "yarn@3.6.3"
}
