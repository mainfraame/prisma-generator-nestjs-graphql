{
  "name": "prisma-generator-ql",
  "description": "Provide a description about your generator",
  "version": "1.0.0",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-generator-ql": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "gen": "prisma generate --schema=./public.prisma",
    "format": "prisma format --schema=./public.prisma",
    "dev": "npx tsc -w",
    "build": "npx tsc --watch",
    "prepack": "yarn build",
    "test": "jest"
  },
  "dependencies": {
    "@nestjs/core": "^9.3.9",
    "@nestjs/apollo": "^10.2.0",
    "@nestjs/graphql": "^10.2.0",
    "@prisma/client": "^4.10.1",
    "@prisma/generator-helper": "^4.10.1",
    "@prisma/sdk": "^4.0.0",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "lodash": "^4.17.21",
    "prettier": "^2.8.4",
    "@swc/core": "^1.3.36",
    "@swc/cli": "^0.1.62",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "27.0.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "17.0.21",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "27.4.7",
    "prisma": "^4.10.1",
    "ts-jest": "27.1.4",
    "typescript": "^4.9.5"
  },
  "homepage": "Link to homepage or github readme here",
  "repository": {
    "type": "git",
    "url": "Repo link here"
  },
  "author": "Your Name <Your Email>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator"
  ],
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "auto",
    "htmlWhitespaceSensitivity": "strict",
    "jsxSingleQuote": true,
    "importOrder": [
      "^@(.*)/(.*)$",
      "<THIRD_PARTY_MODULES>",
      "^[./]"
    ],
    "importOrderCaseInsensitive": true,
    "importOrderParserPlugins": [
      "typescript",
      "decorators-legacy"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "insertPragma": false,
    "quoteProps": "as-needed",
    "requirePragma": false,
    "proseWrap": "preserve",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  }
}
