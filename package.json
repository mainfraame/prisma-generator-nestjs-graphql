{
  "name": "prisma-generator-nestjs-graphql-crud",
  "description": "generates basic graphql crud classes for @nestjs/graphql",
  "version": "2.5.3",
  "bin": "./dist/prisma-generator-nestjs-graphql-crud.js",
  "license": "MIT",
  "author": "dayne.mentier@gmail.com",
  "keywords": [
    "@nestjs/graphql",
    "graphql",
    "graphql-yoga",
    "nestjs",
    "prisma",
    "dto",
    "resolvers",
    "generator"
  ],
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "node ./build.js && yarn build:shebang && yarn build:chmod",
    "build:chmod": "chmod +x ./dist/prisma-generator-nestjs-graphql-crud.js",
    "build:shebang": "echo \"#!/usr/bin/env node\"|cat - ./dist/prisma-generator-nestjs-graphql-crud.js > /tmp/out && mv /tmp/out ./dist/prisma-generator-nestjs-graphql-crud.js",
    "casing": "yarn prisma-case-format --file=./public.prisma",
    "decheck": "yarn depcheck .",
    "dev": "yarn build && yarn gen",
    "format": "yarn prisma format --schema=./public.prisma",
    "gen": "prisma generate --schema=./public.prisma",
    "lint": "eslint --ignore-path ./.eslintignore . --quiet --fix",
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit --incremental false"
  },
  "dependencies": {
    "@prisma/generator-helper": "^5.10.2",
    "@swc/core": "^1.4.2",
    "dataloader": "^2.2.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "@nestjs/graphql": "^12.1.1",
    "@prisma/client": "^5.10.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.6",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "depcheck": "^1.4.7",
    "esbuild": "^0.20.1",
    "esbuild-node-externals": "^1.13.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.5",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "prisma-case-format": "^2.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@nestjs/core": "~10.3.3",
    "@nestjs/graphql": "~12.1.1",
    "graphql": "~16.8.1",
    "graphql-scalars": "~1.22.5"
  },
  "resolutions": {
    "semver": "7.5.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "modulePathIgnorePatterns": [
      "__helpers__/",
      "__fixtures__/"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "auto",
    "htmlWhitespaceSensitivity": "strict",
    "importOrder": [
      "^@(.*)/(.*)$",
      "<THIRD_PARTY_MODULES>",
      "^[./]"
    ],
    "importOrderCaseInsensitive": true,
    "importOrderParserPlugins": [
      "decorators-legacy",
      "jsx",
      "tsx",
      "typescript"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "insertPragma": false,
    "jsxSingleQuote": true,
    "quoteProps": "as-needed",
    "requirePragma": false,
    "proseWrap": "preserve",
    "semi": true,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "packageManager": "yarn@4.0.2"
}
