{
  "jsc": {
    "target": "es2020",
    "parser": {
      "syntax": "typescript",
      "decorators": true
    },
    "transform": {
      "optimizer": {
        "simplify": true
      },
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "keepClassNames": true
  },
  "module": {
    "type": "commonjs",
    "strict": false
  },
  "sourceMaps": "inline",
  "minify": true
}
